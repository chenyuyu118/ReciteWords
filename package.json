{
  "name": "words",
  "version": "0.0.0",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build  &&  electron-builder",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build-only": "vite build",
    "clear": "vite",
    "dev": "vite",
    "electron": "electron .",
    "electron-build": "electron-builder",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "preview": "vite preview --port 4173",
    "start": "electron-forge start",
    "type-check": "vue-tsc --noEmit"
  },
  "main": "dist-electron/mainElectron.js",
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "axios": "^1.1.3",
    "crypto-js": "^4.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "element-plus": "^2.2.19",
    "mongoose": "^6.7.1",
    "node-abi": "^3.28.0",
    "pinia": "^2.0.21",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.11.0",
    "three": "^0.146.0",
    "vue": "^3.2.38",
    "vue-3d-loader": "^2.1.3",
    "vue-router": "^4.1.5",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@electron-forge/maker-deb": "^6.0.1",
    "@electron-forge/maker-rpm": "^6.0.1",
    "@electron-forge/maker-squirrel": "^6.0.1",
    "@electron-forge/maker-zip": "^6.0.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.18.3",
    "@types/three": "^0.146.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "electron": "^21.2.3",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-electron": "^0.10.4",
    "vite-plugin-electron-renderer": "^0.10.4",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-tsc": "^0.40.7"
  },
  "build": {
    "appId": "com.electron.desktop",
    "productName": "electron",
    "asar": true,
    "copyright": "Copyright © 2022 electron",
    "directories": {
      "output": "release/"
    },
    "files": [
      "dist-electron"
    ],
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080"
      }
    ],
    "releaseInfo": {
      "releaseNotes": "版本更新的具体内容"
    }
  }
}
